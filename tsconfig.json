{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@welcome-page/*": ["cypress/POM/*"],
      "@registration-page/*": ["cypress/POM/*"],
      "@login-page/*": ["cypress/POM/*"],
      "@side-navigation/*": ["cypress/POM/*"],
      "@fixtures/*": ["cypress/fixtures/*"]
    }
  },
  "include": ["**/*.ts"]
}